{
    "name": "pyrocms/pyrocms",
    "description": "Build better Laravel websites and applications faster with Pyro.",
    "type": "project",
    "keywords": [
        "cms",
        "pyro",
        "pyrocms",
        "php cms",
        "laravel cms"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "PyroCMS, Inc.",
            "email": "support@pyrocms.com"
        },
        {
            "name": "Ryan Thompson",
            "email": "ryan@pyrocms.com"
        }
    ],
    "require": {
        "facade/ignition": "~1.6.0",
        "wikimedia/composer-merge-plugin": "~1.4.0",
        "anomaly/streams-composer-plugin": "~1.1.0",
        "anomaly/streams-platform": "2.0.x-dev",

        "anomaly/users-module": "3.0.x-dev",
        "anomaly/settings-module": "3.0.x-dev",
        "anomaly/preferences-module": "3.0.x-dev",

        "anomaly/url-field_type": "3.0.x-dev",
        "anomaly/text-field_type": "3.0.x-dev",
        "anomaly/slug-field_type": "3.0.x-dev",
        "anomaly/tags-field_type": "3.0.x-dev",
        "anomaly/email-field_type": "3.0.x-dev",
        "anomaly/select-field_type": "3.0.x-dev",
        "anomaly/boolean-field_type": "3.0.x-dev",
        "anomaly/datetime-field_type": "4.0.x-dev",
        "anomaly/textarea-field_type": "3.0.x-dev",
        "anomaly/multiple-field_type": "3.0.x-dev",
        "anomaly/checkboxes-field_type": "3.0.x-dev",
        "anomaly/relationship-field_type": "3.0.x-dev",

        "anomaly/default_authenticator-extension": "^2.1",
        "anomaly/user_security_check-extension": "^2.1",
        "anomaly/sitemap-extension": "3.0.x-dev",

        "pyrocms/afterburner-theme": "dev-master",



        "anomaly/throttle_security_check-extension": "~2.1.0",
        "anomaly/private_storage_adapter-extension": "2.0.x-dev",
        "anomaly/default_page_handler-extension": "~2.1.0",
        "anomaly/xml_feed_widget-extension": "~2.1.0",
        "anomaly/page_link_type-extension": "~2.1.0",
        "anomaly/url_link_type-extension": "~2.1.0",

        "anomaly/colorpicker-field_type": "3.0.x-dev",
        "anomaly/polymorphic-field_type": "~2.1.0",
        "anomaly/encrypted-field_type": "~2.1.0",
        "anomaly/repeater-field_type": "2.0.x-dev",
        "anomaly/language-field_type": "~2.2.0",
        "anomaly/markdown-field_type": "4.0.x-dev",
        "anomaly/wysiwyg-field_type": "4.0.x-dev",
        "anomaly/country-field_type": "3.0.x-dev",
        "anomaly/decimal-field_type": "3.0.x-dev",
        "anomaly/integer-field_type": "~2.1.0",
        "anomaly/editor-field_type": "4.0.x-dev",
        "anomaly/slider-field_type": "4.0.x-dev",
        "anomaly/addon-field_type": "3.0.x-dev",
        "anomaly/state-field_type": "~2.3.0",
        "anomaly/files-field_type": "3.0.x-dev",
        "anomaly/file-field_type": "~2.2.0",

        "anomaly/configuration-module": "3.0.x-dev",
        "anomaly/navigation-module": "3.0.x-dev",
        "anomaly/dashboard-module": "3.0.x-dev",
        "anomaly/redirects-module": "3.0.x-dev",
        "anomaly/variables-module": "3.0.x-dev",
        "anomaly/addons-module": "3.0.x-dev",
        "anomaly/blocks-module": "2.0.x-dev",
        "anomaly/search-module": "4.0.x-dev",
        "anomaly/system-module": "2.0.x-dev",
        "anomaly/pages-module": "3.0.x-dev",
        "anomaly/posts-module": "3.0.x-dev",
        "anomaly/files-module": "3.0.x-dev",

        "anomaly/contact-plugin": "~1.2.0",
        "anomaly/helper-plugin": "~2.1.0",

        "anomaly/robots-extension": "~2.1.0",
        "anomaly/html_block-extension": "2.0.x-dev",
        "anomaly/wysiwyg_block-extension": "2.0.x-dev",

        "pyrocms/starter-theme": "~1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "fzaninotto/faker": "^1.8",
        "mockery/mockery": "0.9.*",
        "symfony/css-selector": "3.1.*",
        "symfony/dom-crawler": "3.1.*",
        "anomaly/installer-module": "3.0.x-dev"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.pyrocms.com"
        },
        {
            "type": "composer",
            "url": "https://community.pyrocms.com"
        },
        {
            "type": "composer",
            "url": "https://development.pyrocms.com"
        }
    ],
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "addons/*/*/*/composer.json",
                "core/*/*/composer.json",
                "vendor/anomaly/streams-platform/composer.json"
            ],
            "recurse": true,
            "replace": false
        },
        "laravel": {
            "dont-discover": [
                "*"
            ]
        }
    },
    "config": {
        "bin-dir": "bin",
        "preferred-stable": false,
        "minimum-stability": "dev",
        "preferred-install": "dist",
        "optimize-autoloader": true
    }
}
